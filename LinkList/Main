public class Main {
    public static void main(String[] args) {
        LinkedList<Integer> list = new LinkedList<>();

        list.add(1);
        list.add(2);
        list.add(3);

        list.printList(); // Output: 1 2 3

        list.remove(2);

        list.printList(); // Output: 1 3

        list.removeAt(1);

        list.printList(); // Output: 1
    }
}
